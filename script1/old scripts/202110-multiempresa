
--Separar grupos
alter table co_grupo add column co_empresa integer ;
alter table co_grupo add FOREIGN KEY (co_empresa) REFERENCES co_empresa(id);
update co_grupo set co_empresa = 1;
alter table co_grupo alter column co_empresa set not null;


--Separar tipos de gasto
alter table cat_tipo_gasto add column co_empresa integer ;
alter table cat_tipo_gasto add FOREIGN KEY (co_empresa) REFERENCES co_empresa(id);
alter table cat_tipo_gasto add column sistema boolean not null default false  ;
update cat_tipo_gasto set co_empresa = 1,sistema=false where id > 10;
update cat_tipo_gasto set sistema=true where id < 10;

--Separar cargos
alter table cat_cargo add column co_empresa integer ;
alter table cat_cargo add FOREIGN KEY (co_empresa) REFERENCES co_empresa(id);
update cat_cargo set co_empresa = 1 where id >= 1000 ;


-- template

alter table co_empresa add column co_template integer not null default 1;
alter table co_empresa add FOREIGN KEY (co_template) REFERENCES co_template(id);

--clonar el template de magic al id=2
insert into co_template(co_empresa,nombre, encabezado,pie,genero)
values(1,'Template 1',
	  (select encabezado from co_template where id = 1),
	  (select pie from co_template where id = 1),
	  1);

--Relacionar el template creado a magic
update co_empresa set co_template = 2 where id=1

