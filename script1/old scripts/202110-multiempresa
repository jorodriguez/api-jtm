update cat_cargo set sistema = false where id > 4;


--- modificaciones para el nuevo proceso de calculo de horas extras y cargo extra

alter table cat_cargo add column identificador varchar(16);  
COMMENT ON COLUMN cat_cargo.identificador IS 'Sirve para identificar el tipo de cargo por empresa, por ejemplo al cálcular las horas extras (HE) o el cargo extra (CA)';

update cat_cargo set identificador = 'TIEX' where id =3;
update cat_cargo set identificador = 'CAEX' where id =4;

update cat_cargo set sistema = false,
					co_empresa = 1
					where id in (3,4);

alter table co_sucursal add column calcular_recargos boolean default false;
update co_sucursal set calcular_recargos =  true where id in (1,2,3);

alter table cat_servicio add column co_empresa integer ;
alter table cat_servicio add FOREIGN KEY (co_empresa) REFERENCES co_empresa(id);
alter table cat_servicio add column sistema boolean not null default false;
update cat_servicio set co_empresa = 1;



