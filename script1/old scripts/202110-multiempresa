--Separar familiares
alter table co_familiar add FOREIGN KEY (co_empresa) REFERENCES co_empresa(id);
update co_familiar set co_empresa = 1;
alter table co_familiar alter column co_empresa set not null;


--para el correo de bienvenida


---

drop table configuracion;
 create table configuracion(
			id serial not null primary key,
			link_descarga_app_android text	
			
 );
 
 alter table configuracion add column url_pagina_oficial text;
alter table configuracion add column eliminado boolean not null default false;
alter table configuracion add column sistema boolean default false;
alter table configuracion add column co_empresa integer;
alter table configuracion add FOREIGN KEY (co_empresa) REFERENCES co_empresa(id);

alter table configuracion add column genero integer;
alter table configuracion add FOREIGN KEY (genero) REFERENCES usuario(id);

alter table configuracion add column modifico integer;
alter table configuracion add FOREIGN KEY (modifico) REFERENCES usuario(id);

alter table configuracion add column fecha_genero timestamp not null default current_timestamp;
alter table configuracion add column fecha_modifico timestamp ;

insert into configuracion(link_descarga_app_android,url_pagina_oficial,co_empresa,sistema,genero)
values('https://drive.google.com/file/d/17OizhnQAaxhFGhVq12x0PpkMTUodpL9N','https://google.com',null,true,1)


-- configiracion de magic
insert into configuracion(link_descarga_app_android,url_pagina_oficial,co_empresa,genero)
values('https://drive.google.com/file/d/17OizhnQAaxhFGhVq12x0PpkMTUodpL9N','https://magicintelligence.com',1,1)



-- configiracion de magic
insert into configuracion(link_descarga_app_android,url_pagina_oficial,co_empresa,genero)
values('https://drive.google.com/file/d/17OizhnQAaxhFGhVq12x0PpkMTUodpL9N','https://magicintelligence.com',1,1)

alter table co_empresa add column configuracion integer default 1;
alter table co_empresa add FOREIGN KEY (configuracion) REFERENCES configuracion(id);

-- configuracion para magic
update co_empresa set configuracion = 2 where id = 1;

--
alter table configuracion add column logotipo text;

---configuracion para envio 
--ṕara magic
alter table configuracion add column configuracion_correo text;

--para confuracion por defecto para info de family connect





--- para enviar el correo la cuenta de envio
alter table configuracion add column remitente_from text;

update configuracion set  
	co_empresa= null, 
  remitente_from = 'Family Connect<familyconnectinfo1@gmail.com>',
  configuracion_correo=
'{
      "host":"smtp.gmail.com",
      "port":465,
      "secureConnection":true,
      "auth":{
         "user":"familyconnectinfo1@gmail.com",
         "pass":"Armincesa1"
      },
      "tls":{
         "ciphers":"SSLv3"
      }
   }'
where id = 1;

